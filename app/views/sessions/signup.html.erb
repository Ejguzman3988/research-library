<h1> Sign Up Form </h1>

 <%= form_for @user, url: '/create'  do |f| %>

    <%= f.label :email %>
    <%= f.email_field :email%>
    <br>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <br>
    <%= f.label :age %>
    <%= f.number_field :age %>
    <br>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <br>
    <%= f.submit 'Sign Up' %>
<% end %>